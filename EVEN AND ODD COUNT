#include<stdio.h>
int main(){
int i,j,a[3][3];
printf("enter the matrix:\n");
for(i=0;i<3;i++)
{
for(j=0;j<3;j++)
{
scanf("%d",&a[i][j]);
}
}
int ec=0,oc=0;
for(i=0;i<3;i++)
{
for(j=0;j<3;j++)
{
if(a[i][j]%2==0)
{
ec++;
}
else
{
oc++;
}
}
}
printf("sum of even num:%d",ec);
printf("sum of odd num:%d",oc);
return 0;
}
